<section class='debunk'>
  {{#each copy.debunk-intro}}
    <p class='prose'>{{{value}}}</p>
  {{/each}}

  {{#each copy.debunk}}
    <div class='debunk-wrapper'>
      <h3 class='section-hed tk-atlas'>{{title}}</h2>
      <p class='prose'>{{text}}</p>
      <div class='debunk-ui'>
        <span class='debunk-ui_instructions tk-atlas'>Highlight The</span>
        <div class='debunk-ui_button button-average'><button class='btn small is-active'>Average</button></div>
        <div class='debunk-ui_button button-biggest'><button class='btn small'>Biggest</button></div>
        <div class='debunk-ui_button button-smallest'><button class='btn small'>Smallest</button></div>
        <span class='debunk-ui_instructions tk-atlas'>Pockets</span>
      </div>

      <figure class='debunk-graphic' data-location='{{location}}'></figure>
    </div>
  {{/each}}
</section>
