.debunk
  .debunk-graphic
    display flex
    flex-wrap wrap
    max-width 900px
    margin 4rem auto

    .chart-label
      text-transform uppercase
      font-family $sans
      font-weight 600

    .container
      width 225px
      height 300px

      svg
        stroke-width 2px
        stroke $dark-denim
        stroke-opacity 0.1
        fill none
