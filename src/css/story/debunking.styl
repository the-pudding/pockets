.debunk
  .debunk-graphic
    display flex
    flex-wrap wrap
    max-width 900px
    margin 4rem auto
    justify-content: space-around;

    .chart-label
      text-transform uppercase
      font-family $sans
      font-weight 600

    .container
      //max-width 225px
      height 300px
      min-width 125px

      svg
        stroke-width 2px
        stroke $dark-denim
        stroke-opacity 0.1
        fill none
